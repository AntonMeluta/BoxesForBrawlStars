    \u043D\u0430 \u041F\u0443\u0442\u0438 \u043A \u0441\u043B\u0430\u0432\u0435 \u0437\u0430
    \u0434\u043E\u0441\u0442\u0438\u0436\u0435\u043D\u0438\u0435 6000 \u0442\u0440\u043E\u0444\u0435\u0435\u0432
    \u041E\u0431\u043B\u0430\u0434\u0430\u0435\u0442 \u0441\u0440\u0435\u0434\u043D\u0438\u043C
    \u043A\u043E\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E\u043C \u0437\u0434\u043E\u0440\u043E\u0432\u044C\u044F
    \u0438 \u0432\u044B\u0441\u043E\u043A\u0438\u043C \u0443\u0440\u043E\u043D\u043E\u043C.
    \u041E\u043D \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u0443\u0435\u0442 \u043B\u0430\u0437\u0435\u0440\u043D\u044B\u0439
    \u043F\u0438\u0441\u0442\u043E\u043B\u0435\u0442, \u0438\u0437 \u043A\u043E\u0442\u043E\u0440\u043E\u0433\u043E
    \u0441\u0442\u0440\u0435\u043B\u044F\u0435\u0442 \u043B\u0443\u0447\u0430\u043C\u0438
    \u043F\u043E \u0432\u0440\u0430\u0433\u0430\u043C. \u042F\u0432\u043B\u044F\u0435\u0442\u0441\u044F
